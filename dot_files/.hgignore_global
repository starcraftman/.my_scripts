# This file is a global file to ignore things that probably shouldn't
# be committed to git. Mirror of my gitconfig ignore.
#
# In order to use this file, copy/link it to ~/.gitignore_global, and add the
# following to your "~/.hgrc" file.
#
# [ui]
# ignore = ~/.hgignore_global
syntax: glob

# Compiled source #
###################
*.com
*.class
*.dll
*.exe
*.o
*.pyc
*.so

# Archive #
###########
# it's better to unpack these files and commit the raw source
# git has its own built in compression methods
*.tar
*.gz
*.bz2
*.7z
*.rar
*.zip
*.deb
*.rpm
*.dmg
*.jar
*.iso

# Logs and databases #
######################
*.log
*.sql
*.sqlite

# OS generated files #
######################
.DS_Store
.DS_Store?
._*
.Spotlight-V100
.Trashes
ehthumbs.db
Thumbs.db

# Extra #
#########
.ycm_extra_conf.py*
*.kdev4
*-swp
*.swp
/build
*.pro

